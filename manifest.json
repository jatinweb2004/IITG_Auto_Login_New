{
  "manifest_version": 3,
  "name": "IITG AutoLogin 2.0 (Refreshed)",
  "description": "Automatically log in to IITG network. Refreshed UI/CSS; same functionality.",
  "version": "2.0.1",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "notifications",
    "activeTab"
  ],
  "host_permissions": [
    "https://agnigarh.iitg.ac.in:1442/*"
  ],
  "action": {
    "default_title": "IITG AutoLogin 2.0",
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "128": "icon.png"
  }
}